  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "test": "vitest run --dir src/http/sevices",
    "test:watch": "vitest --dir src/http/services --watch",
